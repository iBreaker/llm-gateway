{
  "name": "llm-gateway",
  "version": "0.1.0",
  "description": "智能大语言模型网关服务 - 为 Claude Code 和 Gemini CLI 提供统一代理服务",
  "main": "index.js",
  "scripts": {
    "dev": "next dev -H 0.0.0.0",
    "build": "prisma generate && next build",
    "postinstall": "prisma generate",
    "start": "next start -H 0.0.0.0",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "push": "./scripts/quick-push.sh",
    "deploy": "npm run build && ./scripts/quick-push.sh \"部署更新\"",
    "init-config": "ts-node --project tsconfig.scripts.json scripts/init-config.ts",
    "config:show": "ts-node --project tsconfig.scripts.json -e \"import('./src/lib/config').then(c => c.getAllConfigs().then(console.log))\"",
    "config:clear-cache": "ts-node --project tsconfig.scripts.json -e \"import('./src/lib/config').then(c => c.clearConfigCache())\"",
    "create-admin": "ts-node --project tsconfig.scripts.json scripts/create-admin.ts"
  },
  "keywords": [
    "llm",
    "gateway",
    "claude",
    "gemini",
    "proxy",
    "api",
    "next.js"
  ],
  "author": "LLM Gateway Team",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^6.13.0",
    "@supabase/auth-ui-react": "^0.4.7",
    "@supabase/auth-ui-shared": "^0.1.8",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.53.0",
    "@types/better-sqlite3": "^7.6.0",
    "@vercel/postgres": "^0.10.0",
    "axios": "^1.7.0",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^11.0.0",
    "crypto": "^1.0.1",
    "date-fns": "^3.6.0",
    "google-auth-library": "^9.11.0",
    "https-proxy-agent": "^7.0.0",
    "jose": "^5.6.0",
    "lucide-react": "^0.536.0",
    "nanoid": "^5.0.0",
    "next": "^14.2.0",
    "node-cron": "^4.2.1",
    "postgres": "^3.4.7",
    "prisma": "^6.13.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "socks-proxy-agent": "^8.0.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.0",
    "@testing-library/react": "^16.0.0",
    "@types/bcryptjs": "^2.4.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.9",
    "@types/node-cron": "^3.0.11",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "autoprefixer": "^10.4.0",
    "babel-jest": "^30.0.5",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.0",
    "prettier": "^3.3.0",
    "tailwindcss": "^3.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}
